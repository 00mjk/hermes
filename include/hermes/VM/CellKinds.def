// This file defines all possible kinds of objects that are known to the
// garbage collector.

#ifndef CELL_KIND
#define CELL_KIND(name, jsClassName)
#endif

#ifndef CELL_RANGE
#define CELL_RANGE(rangeName, first, last)
#endif

CELL_KIND(Uninitialized, "")
CELL_KIND(FillerCell, "")
CELL_KIND(DynamicUTF16StringPrimitive, "")
CELL_KIND(DynamicASCIIStringPrimitive, "")
CELL_KIND(ExternalUTF16StringPrimitive, "")
CELL_KIND(ExternalASCIIStringPrimitive, "")
CELL_KIND(DictPropertyMap, "")
CELL_KIND(HiddenClass, "")
CELL_KIND(ArrayStorage, "")
CELL_KIND(SegmentedArray, "")
CELL_KIND(Segment, "")
CELL_KIND(PropertyAccessor, "")
CELL_KIND(Environment, "")
CELL_KIND(Object, "Object")
CELL_KIND(HostObject, "Object")
CELL_KIND(Error, "Error")
CELL_KIND(Arguments, "Arguments")
CELL_KIND(Array, "Array")
CELL_KIND(ArrayBuffer, "ArrayBuffer")
CELL_KIND(DataView, "DataView")
#define TYPED_ARRAY(name, type) CELL_KIND(name##Array, #name "Array")
#include "hermes/VM/TypedArrays.def"
CELL_KIND(ArrayIterator, "Array Iterator")
CELL_KIND(HashMapEntry, "HashMapEntry")
CELL_KIND(OrderedHashMap, "OrderedHashMap")
CELL_KIND(Set, "Set")
CELL_KIND(Map, "Map")
CELL_KIND(SetIterator, "Set Iterator")
CELL_KIND(MapIterator, "Map Iterator")
CELL_KIND(WeakMap, "WeakMap")
CELL_KIND(WeakSet, "WeakSet")
CELL_KIND(BooleanObject, "Boolean")
