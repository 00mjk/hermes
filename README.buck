# in hermes checkout dir:

(cd third-party/llvm && BUCK=1 ../../utils/build_llvm.sh)
buck build hermes-repl
./buck-out/gen/tools/repl/hermes-repl#macosx-x86_64

# cross-compile llvm for ios:
# http://llvm.org/docs/GettingStarted.html#cross-compiling-llvm

(cd third-party/llvm && BUCK=1 TARGET_PLATFORM=iphoneos ../../utils/build_llvm.sh)
(cd third-party/llvm && BUCK=1 TARGET_PLATFORM=iphonesimulator ../../utils/build_llvm.sh)

